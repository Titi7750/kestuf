{% extends '@EasyAdmin/page/content.html.twig' %}

{% block page_title %}Ajout d'un évènement
{% endblock %}

{% block page_actions %}
	<a href="{{ path('admin_event') }}" class="btn btn-secondary">
		<i class="fa fa-reply"></i>
		Retour à la liste des évènements
	</a>
{% endblock %}

{% block content_title %}Ajout d'un évènement
{% endblock %}

{% block main %}
	{{ form_start(formEvent) }}
	{{ form_row(formEvent) }}
	<button type="submit" class="btn btn-primary">Ajouter</button>
	{{ form_end(formEvent) }}
{% endblock %}
